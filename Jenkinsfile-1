pipeline {
  agent any
  stages {
    stage('Test') {
      steps {
        sh 'liquibase --version'
      }
    }
   stage('Status') {
      steps {
        sh 'liquibase status --url=jdbc:snowflake:https://byzriaf-wc40121.snowflakecomputing.com/?database=MY_DATABASE&schema=PUBLIC --changeLogFile=./Changelogfile.xml --username=Pranoti16 --password=HarPra1623@'
      }
    }
    stage('Update') {
      steps {
        sh 'liquibase update --url=jdbc:snowflake:https://byzriaf-wc40121.snowflakecomputing.com/?database=MY_DATABASE&schema=PUBLIC --changeLogFile=./Changelogfile.xml --username=Pranoti16 --password=HarPra1623@'
      }
    }
  }
}
