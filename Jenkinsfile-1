pipeline {
  agent any
  stages {
    stage('Test') {
      steps {
        sh 'liquibase --version'
      }
    }
   stage('Status') {
      steps {
        sh 'liquibase status --changelogFile=changelog.xml --url=jdbc:snowflake://byzriaf-wc40121.snowflakecomputing.com/?user=Pranoti16&db=MY_DB&schema=public --username=Pranoti16 --password=HarPra1623@'
      }
    }
    stage('Update') {
      steps {
        sh 'liquibase update --changelogFile=changelog.xml --url=jdbc:snowflake://byzriaf-wc40121.snowflakecomputing.com/?user=Pranoti16&db=MY_DB&schema=public --username=Pranoti16 --password=HarPra1623@'
      }
    }
  }
}
