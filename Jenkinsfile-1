pipeline {
  agent any
  stages {
    stage('Test') {
      steps {
        sh 'liquibase --version'
      }
    }
   stage('Status') {
      steps {
        sh 'liquibase status --changelogFile=changelog.xml,--url=jdbc:snowflake:https://app.snowflake.com/byzriaf/wc40121/#/data/databases/MY_DATABASE/schemas/PUBLIC,username=Pranoti16,password=HarPra1623@'
      }
    }
    stage('Update') {
      steps {
        sh 'liquibase update --changelogFile=changelog.xml,--url=jdbc:snowflake:https://app.snowflake.com/byzriaf/wc40121/#/data/databases/MY_DATABASE/schemas/PUBLIC,username=Pranoti16,password=HarPra1623@'
      }
    }
  }
}
